{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/ts-node",
      "sourceMaps": true,
      "args": ["-r", "tsconfig-paths/register", "${relativeFile}", "t", "--p","--build"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    }
  ]
}
